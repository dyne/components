    "jest": "29",
    "jest-cli": "29",
    "lit": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^22.7.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "semantic-release": "^23.0.8",
    "stencil-tailwind-plugin": "^1.8.0",
    "storybook": "^8.0.8",
    "tailwindcss": "^3.4.3",
